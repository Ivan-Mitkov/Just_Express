<!DOCTYPE html>
<html>
  <head>
    <title>Movie App</title>
    <% include head %>
  </head>
  <body>
    <% include navbar %>

    <div class="container">
      <div class="row">
        <div class="col-sm-12"><h1>Results for <%=searched%></h1></div>
        <div class="col-sm-12">
          <% parsedData.forEach((movie)=>{%>
        	<% if(movie.poster_path){%>

				<div class="col-md-2 col-sm-4 poster">
						<a href="/movie/<%= movie.id %>"><img src="<%=
						imageBaseUrl+movie.poster_path %>"/> <%=movie.title%></a>
					  </div>
			<%}%>        

          <%})%>
        </div>
      </div>
    </div>
  </body>
</html>
